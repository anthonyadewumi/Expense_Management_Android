<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="outlineBox" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeWidth">5px</item>
        <item name="boxStrokeWidthFocused">@dimen/_2sdp</item>
        <item name="strokeColor">@color/colorTextDarkGray</item>
        <item name="android:radius">10dp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/_10ssp</item>
        <item name="boxCornerRadiusTopEnd">@dimen/_10ssp</item>
        <item name="boxCornerRadiusBottomStart">@dimen/_10ssp</item>
        <item name="boxCornerRadiusTopStart">@dimen/_10ssp</item>

    </style>
</resources>